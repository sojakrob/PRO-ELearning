@using ELearning.Utils
@model ELearning.Models.Data.FormInstanceModel

@{
    Layout = "";
}

<html>
<head>
    <title>Filling In Progress</title>

    @*TODO Move to own Control*@
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/FormInstance/FillingInProgress.js")" type="text/javascript"></script>

    <link href="@Url.Content("~/Content/favicon.png")" rel="shortcut icon" />
    <link href="@Url.Content("~/Content/favicon.png")" rel="icon" type="image/png" />

    
    @if (ViewBag.OpenWindow != null && ViewBag.OpenWindow)
    {
        <script type="text/javascript">
            openFillingFormWindow(@Model.ID);
        </script>
    }

</head>

<body>

    <h1>Filling In Progress</h1>
   
    @Html.ButtonJSClick("Reopen the test window", "Reopen window", string.Format("openFillingFormWindow({0})", Model.ID))

</body>
</html>

