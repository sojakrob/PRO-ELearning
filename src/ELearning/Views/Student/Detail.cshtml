@model ELearning.Models.Data.StudentFormsModel

@{
    ViewBag.Title = "Student view";
}

@if (ViewBag.CurrentUserModel.Type == ELearning.Data.Enums.UserTypes.Administrator || ViewBag.CurrentUserModel.Type == ELearning.Data.Enums.UserTypes.Lector)
{ 
    <h2>@Model.Name</h2>
}

<ul>
@foreach (var group in Model.AssignedGroups)
{
    <li>@group.Name</li>
}
</ul>

@Html.Partial("_FilledForms", Model.FilledForms)
